
record(bo, "$(P)$(R)$(CHID)Enabled")
{		
	field(DESC, "Enable channel.")
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(N),$(TIMEOUT))CH_ENABLED")
}

record(bi, "$(P)$(R)$(CHID)Enabled_RBV")
{		
	field(DESC, "Is channel enabled.")
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),$(N),$(TIMEOUT))CH_ENABLED")
   	field(SCAN, "I/O Intr")
}

## Triggering
record(mbbo, "$(P)$(R)$(CHID)SelfTrgMode")
{
    field(DESC, "Self trigger mode")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(N),$(TIMEOUT))CH_SELF_TRG_MODE")
   	field(ZRST, "DISABLED")
   	field(ONST, "EXTOUT_ONLY")
   	field(TWST, "ACQ_ONLY")
   	field(THST, "ACQ_AND_EXTOUT")

}

